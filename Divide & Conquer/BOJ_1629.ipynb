      "source": [
        "a, b, m = map(int, input().split())\n",
        "\n",
        "def pow(a, b, m):\n",
        "    if b == 1:\n",
        "        return a % m\n",
        "    \n",
        "    val = pow(a, b//2, m)\n",
        "    \n",
        "    if(b%2 ==0):\n",
        "        return val * val % m\n",
        "    return val * val * a % m\n",
        "\n",
        "pow(a,b,m)"
      ],
